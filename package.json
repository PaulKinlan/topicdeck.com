{
  "name": "topicdeck-site",
  "version": "1.0.2",
  "description": "Topicdeck site",
  "main": "index.js",
  "scripts": {
    "build:serviceworker": "node node_modules/topicdeck/tools/producemanifest.js --inputpath=topicdeck/dist/server/public/ --overridepath=./public/ --input=topicdeck/dist/server/public/sw.src.js --output=./public/sw.js",
    "build:copy-assets": "mkdir -p topicdeck/dist/server && cp -R node_modules/topicdeck/dist/server/ topicdeck/dist/server/",
    "now-build": "npm run build:copy-assets && npm run build:serviceworker"
  },
  "keywords": [
    "topicdeck"
  ],
  "author": "Paul Kinlan",
  "license": "Apache-2.0",
  "dependencies": {
    "now": "^15.0.0",
    "topicdeck": "0.0.30"
  }
}

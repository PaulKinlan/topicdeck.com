{
  "version": 2,
  "name": "topicdeck",
  "alias": [
    "topicdeck.com",
    "pwa.topicdeck.com",
    "wasm.topicdeck.com",
    "javascript.topicdeck.com",
    "webdev.topicdeck.com",
    "css.topicdeck.com",
    "chromiumdev.topicdeck.com",
    "webbrowsereng.topicdeck.com",
    "firefoxdev.topicdeck.com",
    "beakerbrowserdev.topdicdeck.com",
    "webkitdev.topicdeck.com"
  ],
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": ["config/*.json", "public/**/*", "topicdeck/dist/server/**/*"]
      }
    }
  ]
}
{
  "version": 2,
  "name": "topicdeck",
  "alias": [
    "topicdeck.com",
    "pwa.topicdeck.com",
    "wasm.topicdeck.com",
    "javascript.topicdeck.com",
    "webdev.topicdeck.com",
    "css.topicdeck.com",
    "chromiumdev.topicdeck.com",
    "webbrowsereng.topicdeck.com",
    "firefoxdev.topicdeck.com",
    "beakerbrowserdev.topdicdeck.com",
    "webkitdev.topicdeck.com"
  ],
  "routes": [
    { "src": "/sw.js", "dest": "/topicdeck/server/public/sw.js" },
    { "src": "/styles/(.*)", "dest": "/topicdeck/server/public/styles/$1" },
    { "src": "/scripts/(.*)", "dest": "/topicdeck/server/public/scripts/$1" },
    { "src": "/images/(.*)", "dest": "/topicdeck/server/public/images/$1" },
    { "src": "/proxy", "dest": "index.js" },
    { "src": "/(.*)", "dest": "index.js" }
  ],
  "builds": [
    {
      "src": "topicdeck/**/*",
      "use": "@now/static"
    },
    {
      "src": "index.js",
      "use": "@now/node-server",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": [
          "config/*.json",
          "public/**/*",
          "topicdeck/server/**/*"
        ]
      }
    }
  ]
}